{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^test"],
      "cache": false
    }
  }
}