{
  "name": "labelbits",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "rm -f ./pnpm-lock.yaml && find . -type d \\( -name \"node_modules\" -o -name \"dist\" -o -name \".turbo\" -o -name \"build\" -o -name \".snowpack\" \\) -prune -exec rm -rf '{}' +",
    "dev": "turbo run dev",
    "format": "npx prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "start": "turbo run start",
    "test": "turbo run test"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.8.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.8.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.2.4",
    "turbo": "^2.1.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "fabric": "^6.0.2"
  },
  "packageManager": "pnpm@9.9.0"
}